#include<bits/stdc++.h>
using namespace std;
int main() {

	// Write your code here
    int n,c=1;
    cin >> n;
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n-i;j++)
        {
            if(j%2==0)
                cout<<"*";
            else {
                cout<<c;
                c++;
            }
        }
        cout<<endl;
    }
    return 0;
}
